#tour
    min-height: 400px;
    position: relative;

    .container
        @media screen and (max-width: 991px)
            position: inherit;

        @media screen and (min-width: 992px)
            width: 100%;

    .macbook-screenshot
        height: 320px;
        max-width: 100%;

        @media screen and (min-width: 992px)
            @media screen and (max-width: 1200px)
                left: -110px;
                position: absolute;
                top: 0;
